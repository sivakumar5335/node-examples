{
  "name": "@toolpad/core",
  "version": "0.12.1",
  "author": "Toolpad Team",
  "description": "Dashboard framework powered by MUI.",
  "main": "./index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "mui-modern": {
        "types": "./modern/index.d.ts",
        "default": "./modern/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./*/index.d.ts",
        "default": "./*/index.js"
      },
      "import": {
        "types": "./esm/*/index.d.ts",
        "default": "./esm/*/index.js"
      },
      "mui-modern": {
        "types": "./modern/*/index.d.ts",
        "default": "./modern/*/index.js"
      }
    },
    "./esm": null,
    "./modern": null,
    "./package.json": "./package.json"
  },
  "keywords": [
    "react",
    "mui",
    "toolpad",
    "internal tools",
    "crud",
    "admin",
    "dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/toolpad.git",
    "directory": "packages/toolpad-core"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/toolpad/issues"
  },
  "homepage": "https://github.com/mui/toolpad#readme",
  "dependencies": {
    "@babel/runtime": "^7.26.9",
    "@mui/utils": "7.0.0-alpha.2",
    "@vitejs/plugin-react": "4.3.4",
    "client-only": "^0.0.1",
    "invariant": "2.2.4",
    "path-to-regexp": "6.3.0",
    "prop-types": "15.8.1",
    "@toolpad/utils": "0.12.1"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.16.6 || ^6.4.0 || ^7.0.0-alpha",
    "@mui/material": "^5.16.6 || ^6.4.0 || ^7.0.0-alpha",
    "next": "^14 || ^15",
    "react": "^18 || ^19",
    "react-router": "^7"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react-router": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "private": false,
  "types": "./index.d.ts"
}